<Window x:Class="Alto_IT.affichageProjets"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Alto_IT"
        mc:Ignorable="d"
        Title="affichageProjets" Height="450" Width="300">
    <Grid RenderTransformOrigin="0.529,0.502" HorizontalAlignment="Left" Width="300">
        <TextBox Name="projetName"  HorizontalAlignment="Left" Margin="59,45,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="33" Width="179"/>
        <ComboBox Name="comboboxProvider" HorizontalAlignment="Left" Margin="59,138,0,0" VerticalAlignment="Top" Width="179">
            <TextBlock Text="Amazon Web Service"></TextBlock>
            <TextBlock Text="Azure"></TextBlock>
            <TextBlock Text="Google Cloud Service"></TextBlock>
        </ComboBox>
        <Button Name="validerModif" Content="Valider" HorizontalAlignment="Left" Margin="59,302,0,0" VerticalAlignment="Top" Width="179" Height="58" Click="ValiderModif_Click"/>

    </Grid>
</Window>
